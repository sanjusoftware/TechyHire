<head>
  <title>TechyHire</title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
</head>
<body>
<!--header -->
<div id="header-wrap">
  <div id="header">
    <div id="nav">
      <ul>
        <li id=<%= @current.eql?("home") ? "current" : ""%>><a href="/">Home</a></li>
        <li id=<%= @current.eql?("companies") ? "current" : ""%>><%= link_to 'Companies', companies_path %></li>
        <li id=<%= @current.eql?("jobs") ? "current" : ""%>><%= link_to 'Jobs', jobs_path %></li>
        <li><a href="/">Support</a></li>
        <li><a href="/">About</a></li>
        <% if user_signed_in? %>
            <li>
              <%= link_to "Sign Out", destroy_user_session_path %>
            </li>
        <% else %>
            <li>
              <%= link_to "Sign In", new_user_session_path %>
            </li>
            <li>
              <%= link_to "Sign Up", new_user_registration_path %>
            </li>
        <% end %>
      </ul>
    </div>
  </div>
</div>

<!-- content-outer -->
<div id="content-wrap" class="clear">

  <!-- content -->
  <div id="content">

    <% for name in [:warning, :alert, :error, :notice] %>
        <% if flash[name] %>
            <div id=<%= name %>><%= flash[name] %></div>
        <% end %>
    <% end %>
    <br/>
    <%= yield %>
    <!-- content -->
  </div>

  <!-- /content-out -->
</div>
